{
	"name": "@time-input-polyfill/utils",
	"version": "1.0.4",
	"description": "A library of helper functions designed to make the building of time input polyfill components easier.",
	"main": "npm/index.js",
	"types": "npm/time-input-polyfill-utils.d.ts",
	"browser": "npm/time-input-polyfill-utils.js",
	"jsdelivr": "npm/time-input-polyfill-utils.min.js",
	"scripts": {
		"start": "cypress open",
		"test": "cypress run",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"cy:entryLog": "cypress run --spec \"cypress/integration/ManualEntryLog.test.ts\"",
		"cy:converters": "cypress run --spec \"cypress/integration/converters.test.ts\"",
		"cy:getters": "cypress run --spec \"cypress/integration/getters.test.ts\"",
		"cy:modifiers": "cypress run --spec \"cypress/integration/modifiers.test.ts\"",
		"cy:is": "cypress run --spec \"cypress/integration/is.test.ts\"",
		"clean": "npx rimraf npm build",
		"build:types": "npx rollup -c rollup.config.types.js",
		"build": "npm run clean npm && npm run build:types && npx rollup -c",
		"prepublishOnly": "npm run build && npx tsc --project tsconfig.npm.json",
		"postpublish": "npm run clean",
		"testpublish": "npm run prepublishOnly && npm run postpublish",
		"pub": "npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dan503/time-input-polyfill-utils.git"
	},
	"keywords": [
		"time",
		"input",
		"polyfill",
		"helpers",
		"utility",
		"util",
		"library"
	],
	"author": "Daniel <dan503.npm@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Dan503/time-input-polyfill-utils/issues"
	},
	"homepage": "https://github.com/Dan503/time-input-polyfill-utils#readme",
	"dependencies": {
		"browser-monads-ts": "^2.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-external-helpers": "^7.16.11",
		"@babel/preset-env": "^7.16.11",
		"@cypress/webpack-preprocessor": "^5.0.0",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^2.25.0",
		"@typescript-eslint/parser": "^2.25.0",
		"babel-loader": "^8.1.0",
		"cypress": "^9.5.1",
		"cypress-promise": "^1.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-prettier": "^3.1.2",
		"prettier": "^2.3.2",
		"prettier-plugin-organize-imports": "^2.3.3",
		"rollup": "^2.69.1",
		"rollup-plugin-dts": "^4.2.3",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-terser": "^7.0.2",
		"ts-loader": "^9.4.0",
		"ts-node": "^8.8.2",
		"tslib": "^2.0.3",
		"typescript": "^4.6.2",
		"webpack": "^5.0.0"
	}
}
